div.col-sm-12.main
	.col-sm-12.tab
		ul.nav.nav-pills.ivsetpills
			li.active
				a.list(data-toggle='pill', data-url='/listivset') View
			li
				a.add(data-toggle='pill', data-url='/addivset') Add 
	.col-sm-12.cnt
		each ivset in ivsets
			div.col-sm-12.col-xs-12.list-ivset
				div.col-sm-5.col-xs-4.iv1
					h4 #{ivset.ivname}
				div.col-sm-5.col-xs-4.iv2
					h4 #{ivset.ivdpf}
				.col-sm-1.col-xs-2.x1
					span.glyphicon.glyphicon-pencil
				.col-sm-1.col-xs-2.x2
					span.glyphicon.glyphicon-remove
