.col-xs-12.hidden-sm.hidden-md.hidden-lg &nbsp 
.col-xs-12.hidden-sm.hidden-md.hidden-lg &nbsp 
div.col-sm-12.col-lg-12.col-xs-12.main
	.col-sm-12.col-lg-12.col-xs-12.tab
		ul.nav.nav-pills.ivsetpills
			li.active
				a.list(data-toggle='pill', data-url='/listpatient') View
			li
				a.add(data-toggle='pill', data-url='/addpatient') Add 
	.col-sm-12.col-lg-12.col-md-12.col-xs-12.cnt
		each patient in patients 
			div.col-sm-12.col-md-12.col-lg-12.col-xs-11.list-patient
				div.col-sm-6.col-md-6.col-lg-6.col-xs-12.n1
					h4 #{patient.name}
				div.col-sm-2.col-md-2.col-lg-2.col-xs-2.a1
					h4 #{patient.age}
				div.col-sm-2.col-md-2.col-lg-2.col-xs-2.w1
					h4 #{patient.weight}
				.col-sm-1.col-md-1.col-lg-1.col-xs-2.m1.btn-default.editpatient(data-url='/editpatient?bed=#{patient._bed}')
					h4.glyphicon.glyphicon-pencil		
				.col-sm-1.col-md-1.col-lg-1.col-xs-2.m1.btn-default
					h4.glyphicon.glyphicon-remove

